library(readxl)
cites <- read_excel("Inspection_Observations_FY19_0.xlsx", sheet="Devices")
cites %>%
top_n(20,Frequency) %>%
ggplot() +
geom_bar(aes(x=reorder(`Short Description`,Frequency),y=Frequency),fill="#00c0ff",
stat="identity",
